{
  "name": "weathery",
  "version": "0.1.0",
  "description": "mini weather report",
  "main": "index.js",
  "scripts": {
    "postinstall": "webpack",
    "dev": "nodemon --watch server server/index & webpack --watch",
    "start": "node server/index",
    "test": "ava -v --serial 'tests/**/*.test.js'",
    "watch": "nodemon -e js,jsx -x npm test",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "knex": "knex"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chhschou/weathery.git"
  },
  "keywords": [
    "weather"
  ],
  "author": "Chris Chou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chhschou/weathery/issues"
  },
  "homepage": "https://github.com/chhschou/weathery#readme",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^2.9.1",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "nock": "^9.0.14",
    "nodemon": "^1.11.0",
    "redux-mock-store": "^1.2.3",
    "sqlite3": "^3.1.9",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^7.4.3",
    "jsx-loader": "^0.13.2",
    "knex": "^0.13.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sodium": "^2.0.1",
    "superagent": "^2.2.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
